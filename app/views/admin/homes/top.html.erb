<div class="container">
  <h2>会員一覧</h2>

   <table class="table">
     <thead>
       <tr>
         <th>登録日</th>
         <th>image</th>
         <th>名前</th>
         <th>いいね数</th>
         <th colspan="3"></th>
       </tr>
     </thead>
     <tbody>
      <% @users.each do |user| %>
       <tr>
        <!--会員の登録日時を表示させたい-->
        <td class="scoreStatus_list_label registration_date">
         <%= user.created_at.strftime('%Y/%m/%d') %>
        </td>
        <td>
         <%= image_tag user.get_profile_image(50,50) %>
        </td>
        <td><%= user.name %></td>
        <td></td>
        <td>
          <%= link_to "詳細", admin_user_path(user.id), class: "btn btn-primary" %>
        </td>
      </tr>
          <% end %>
     </tbody>
   </table>
</div>
